{
  "name": "@blockwatch/hicetnunc-wrapper",
  "description": "Tezos Hicetnunc Polywrapper",
  "private": true,
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "reset": "yarn clean && yarn build",
    "clean": "npx rimraf ./node_modules ./build ./coverage ./.wrap ./**/wrap",
    "build": "npx polywrap build",
    "codegen": "npx polywrap codegen",
    "codegen:app": "npx polywrap app codegen -m src/__tests__/e2e/types/polywrap.app.yaml -g src/__tests__/e2e/types/wrap",
    "test": "yarn test:e2e",
    "test:e2e": "yarn codegen:app && jest --passWithNoTests --runInBand --verbose",
    "test:env:up": "npx polywrap test-env up",
    "test:env:down": "npx polywrap test-env down",
    "deploy": "npx polywrap deploy"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "polywrap": "0.1.1",
    "@polywrap/client-js": "0.1.1",
    "@polywrap/test-env-js": "0.1.1",
    "@polywrap/wasm-as": "0.1.1",
    "assemblyscript": "0.19.1",
    "jest": "27.4.7",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "assemblyscript-json": "^1.1.0"
  }
}
