{
  "name": "near-polywrap-js",
  "description": "Polywrap TypeScript plugin for NEAR Protocol",
  "license": "MIT",
  "version": "0.0.3",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "schema.graphql"
  ],
  "scripts": {
    "prettier-format": "npx prettier --write ./src",
    "reset": "yarn clean && yarn && yarn build",
    "clean": "npx rimraf ./node_modules ./build ./coverage ./.w3",
    "build": "rimraf ./build && yarn codegen && npx tsc --project tsconfig.build.json",
    "codegen": "npx w3 plugin codegen",
    "test": "jest --passWithNoTests --runInBand --verbose",
    "test:ci": "jest --passWithNoTests --runInBand --verbose",
    "test:watch": "jest --watch --passWithNoTests --verbose"
  },
  "dependencies": {
    "@web3api/core-js": "0.0.1-prealpha.71",
    "bn.js": "5.2.0",
    "js-sha256": "0.9.0",
    "localstorage-polyfill": "^1.0.1",
    "mock-browser": "^0.92.14",
    "near-api-js": "^0.44.2"
  },
  "devDependencies": {
    "@types/bn.js": "5.1.0",
    "@types/jest": "26.0.8",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "@web3api/cli": "0.0.1-prealpha.71",
    "@web3api/client-js": "0.0.1-prealpha.71",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.0",
    "jest": "26.6.3",
    "near-hello": "0.5.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "ts-node": "8.10.2",
    "typescript": "4.0.7",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
